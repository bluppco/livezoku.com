---

// IMPORT ASTRO COMPONENTS
import News from "@/components/home/featured/news/index.astro"

// IMPORT ASTRO ATOMS
import HeadingTwo from "@/atoms/headings/two/index.astro"

// GET ASTRO PROPS
const {

    description1,
    description2,
    description3,
    description4,
    description5,

} = Astro.props

---

<section class="space-y-8">
    <HeadingTwo>AS FEATURED IN</HeadingTwo>
    <section class="flex">
        <News
            alternative_text="Image"
            description={ description1 }
            source="/img.jpeg"
        />
        <News
            alternative_text="Image"
            description={ description2 }
            source="/img.jpeg"
        />
        <News
            alternative_text="Image"
            description={ description3 }
            source="/img.jpeg"
        />
        <News
            alternative_text="Image"
            description={ description4 }
            source="/img.jpeg"
        />
        <News
            alternative_text="Image"
            description={ description5 }
            source="/img.jpeg"
        />
    </section>
</section>
